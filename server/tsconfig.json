{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "allowJs": true,
        "resolveJsonModule": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "./**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}